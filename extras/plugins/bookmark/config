set bookmark.base "{wkb.plugins-dir}/bookmark"
set bookmark.file "{bookmark.base}/bookmarks.html"
alias b "spawn-sync \"{bookmark.base}/bookmark.sh\" \"\{v.uri}\" \"\{v.title}\""
alias bn "spawn-sync \"{bookmark.base}/bookmark.sh\""
alias be "spawn \{term} -e \"{bookmark.base}/edit.sh\" \{editor}"
unset bookmark.base
